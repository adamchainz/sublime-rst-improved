paragraph

	# first a non-indented paragraph
	meta.paragraph.restructuredtext
		(?x) ^ (?=\S.*\w)
		(?x) ^ (?!\S) | ^ (?=[\W\s]*$) | (?=::$)

		#inline

	# indented paragraphs are more fuzzy
	meta.paragraph.restructuredtext
		(?x) ^ (?=({hws}+)\S)

		#inline

		(?x) (?=::$) | $


